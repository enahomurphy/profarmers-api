  "description": "api for wishbox",
  "main": "server.js",
  "scripts": {
    "test": "node_modules/.bin/_mocha test/**/*.js",
    "start": "node server.js",
    "dev": "nodemon server.js",
    "lint": "eslint \"**/*.js\"",
    "migrate": "node_modules/.bin/sequelize db:migrate --url 'postgresql://root:profarmers@postgres/profarmers'",
