  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enahomurphy/wishbox.git"
  },
  "keywords": [
    "wishbox",
    "microservices",
    "api",
    "docker",
    "mongo",
    "nodejs"
  ],
