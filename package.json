  "license": "ISC",
  "bugs": {
    "url": "https://github.com/enahomurphy/wishbox/issues"
  },
  "homepage": "https://github.com/enahomurphy/wishbox#readme",
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "chai": "^4.1.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.7.0",
    "mocha": "^6.2.1",
    "sequelize-cli": "^5.5.1",
    "sinon": "^7.5.0"
  },
  
  "dependencies": {
    "bcryptjs": "^2.4.3",
